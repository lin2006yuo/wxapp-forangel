/**app.wxss**/
@import "./style/common.wxss";
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

.btn {
  width: 678rpx;
  height: 80rpx;
  color: #fff;
  font-size: 32rpx;
  background-color: #fc4969;
}
.formItem {
  line-height: 0;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #eee;
  margin-left: 40rpx;
}
.form_label {
  display: inline-block;
  font-size: 36rpx;
  height: 60rpx;
  line-height: 60rpx;
  vertical-align: top;
}
.inputControl_inline {
  display: inline-block;
  height: 60rpx;
  line-height: 60rpx;
}
.inputControl_inline .form_input {
  line-height: 60rpx;
  height: 60rpx;
}
.holder_class {
  font-size: 36rpx;
}